public class Kata
{
  public static double SumArray(double[] array)
  {
    double sum = 0;
    foreach (var num in array) sum += num;
    return sum;    
  }
}



-----------------Updated Solution Below----------------



public class Kata
{
  public static double SumArray(double[] array)
  {
    double sum = 0;
    
    foreach (var num in array) sum += num;
    
    return sum;
  }
}
